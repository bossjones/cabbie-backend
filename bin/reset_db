#!/bin/zsh

PYTHON=~/.virtualenvs/cabbie/bin/python

dropdb cabbie
createdb cabbie
psql -d cabbie -c "create extension postgis"
psql -d cabbie -c "create extension postgis_topology"
rm cabbie/apps/**/migrations/0*
$PYTHON manage.py makemigrations
$PYTHON manage.py migrate

